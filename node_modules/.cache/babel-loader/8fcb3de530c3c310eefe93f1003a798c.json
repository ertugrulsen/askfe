;AAHZ,CAXa,CAAf;AAsGA,IAAMC,MAAM,GAAG,IAAIxB,SAAJ,CAAc;AAC3ByB,EAAAA,IAAI,EAAE,SADqB;AAE3Bf,EAAAA,MAAM,EAANA,MAF2B;AAG3BgB,EAAAA,eAAe,EAAE,QAHU;AAI3BC,EAAAA,oBAAoB,EAAE;AAJK,CAAd,CAAf;AAOAH,MAAM,CAACI,UAAP,CAAkB,UAACC,EAAD,EAAKC,IAAL,EAAWC,IAAX,EAAoB;AAAA,MAC5Bd,QAD4B,GACfY,EAAE,CAACd,IADY,CAC5BE,QAD4B;AAEpC,MAAMe,WAAW,GAAGjC,KAAK,CAACkC,OAAN,CAAcD,WAAlC;;AAEA,MAAIf,QAAQ,KAAK,IAAb,IAAqB,CAACe,WAAW,CAACE,QAAtC,EAAgD;AAC9C,WAAOH,IAAI,CAAC;AAAEpB,MAAAA,IAAI,EAAE,QAAR;AAAkBwB,MAAAA,KAAK,EAAE;AAAEC,QAAAA,SAAS,EAAEP,EAAE,CAAClB;AAAhB;AAAzB,KAAD,CAAX;AACD;;AACDoB,EAAAA,IAAI;AACL,CARD;AAUA,eAAeP,MAAf","sourcesContent":["import Vue from 'vue'\r\nimport store from '../store'\r\nimport VueRouter from 'vue-router'\r\nimport constant from '../library/constants/constants'\r\nimport Home from '../views/pages/home/Home'\r\nimport DefaultLayout from '../views/layouts/default-layout'\r\n\r\nVue.use(VueRouter)\r\n\r\nfunction lazyLoad(view){\r\n  return() => import(`../views/pages/${view}/${view}.vue`)\r\n}\r\nfunction lazyLoadByModuleAndPage(moduleName, pageName){\r\n  return() => import(`../views/pages/${moduleName}/${pageName}.vue`)\r\n}\r\n\r\nconst routes = [\r\n  {\r\n    path: '/login',\r\n    name: 'login',\r\n    component: lazyLoad('login')\r\n  },\r\n  {\r\n    path: '/register',\r\n    name: 'Register',\r\n    component: lazyLoad('register')\r\n  },\r\n  {\r\n    path: '/',\r\n    component: DefaultLayout,\r\n    children: [\r\n      {\r\n        path: '/',\r\n        name: 'Home',\r\n        component: Home,\r\n        meta: {\r\n          carosel:true\r\n        }\r\n      },\r\n      {\r\n        path: '/profile',\r\n        name: 'Profile',\r\n        component: lazyLoad('profile'),\r\n        meta: {\r\n          needUser:true,\r\n          carosel:false\r\n        }\r\n      },\r\n      {\r\n        path: '/askQuestion',\r\n        name: 'AskQuestion',\r\n        component: lazyLoad('askQuestion'),\r\n        meta: {\r\n          needUser:true,\r\n          carosel:false\r\n        }\r\n      },\r\n      {\r\n        path: '/questionDetail',\r\n        name: 'QuestionDetail',\r\n        component: lazyLoad('questionDetail'),\r\n        meta: {\r\n          carosel:false\r\n        }\r\n      },\r\n      {\r\n        path: '/questionList',\r\n        name: 'QuestionList',\r\n        component: lazyLoad('questionList'),\r\n        meta: {\r\n          carosel:true\r\n        }\r\n      },\r\n      {\r\n        path: '/search',\r\n        name: 'Search',\r\n        component: lazyLoad('search'),\r\n        meta: {\r\n          carosel:false\r\n        }\r\n      },\r\n      {\r\n        path: '/logout',\r\n        name: 'Logout',\r\n        component: lazyLoad('logout'),\r\n        meta: {\r\n        }\r\n      },\r\n      {\r\n        path: '/management',\r\n        name: 'UserManagement',\r\n        component: lazyLoad('management'),\r\n        meta: {\r\n          authorize: [constant.role.UserManagement]\r\n        }\r\n      },\r\n      {\r\n        path: '/user',\r\n        name: 'User',\r\n        component: lazyLoad('user'),\r\n        meta: {\r\n          authorize: [constant.role.Management]\r\n        }\r\n      },\r\n      {\r\n        path: '/company/companydetail',\r\n        name: 'CompanyDetail',\r\n        component: lazyLoadByModuleAndPage('company', 'CompanyDetail'),\r\n        meta: {\r\n          authorize: [constant.role.CompanyAdmin]\r\n        }\r\n      },\r\n      // otherwise redirect to home\r\n      { path: '*', redirect: '/' }\r\n    ]\r\n  }\r\n]\r\n\r\nconst router = new VueRouter({\r\n  mode: 'history',\r\n  routes,\r\n  linkActiveClass: 'active',\r\n  linkExactActiveClass: 'active'\r\n})\r\n\r\nrouter.beforeEach((to, from, next) => {\r\n  const { needUser } = to.meta\r\n  const currentUser = store.getters.currentUser\r\n  \r\n  if (needUser === true && !currentUser.UserName) {\r\n    return next({ path: '/login', query: { returnUrl: to.path } })\r\n  }\r\n  next()\r\n})\r\n\r\nexport default router\r\n"]}]}